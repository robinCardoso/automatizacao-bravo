{
  "VENDA": {
    "primaryKey": [
      "ID",
      "PRODCOD",
      "NNF",
      "DataE",
      "Referencia"
    ],
    "dashboardMapping": {
      "value": "Total Liq",
      "date": "DataE",
      "group": "Grupo",
      "subGroup": "Sub-Grupo"
    }
  },
  "PEDIDO": {
    "primaryKey": [
      "Doc Item",
      "Doc",
      "Data Proc",
      "Ref"
    ],
    "dashboardMapping": {
      "value": "Total Liq R$",
      "date": "Data Proc",
      "group": "Grupo",
      "subGroup": "Sub-Grupo"
    },
    "filteringRules": [
      {
        "field": "Data Proc",
        "operator": "empty"
      },
      {
        "field": "Doc",
        "operator": "equals",
        "value": "TOTAL"
      },
      {
        "field": "Doc",
        "operator": "equals",
        "value": "TOTAIS"
      },
      {
        "field": "*",
        "operator": "contains",
        "value": "TOTAL GERAL"
      },
      {
        "field": "*",
        "operator": "contains",
        "value": "Total Geral"
      }
    ]
  }
}